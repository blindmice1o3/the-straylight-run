<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout_direction_pad_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    tools:context=".accelerometer.game.gameconsole.gamepad.directionpad.DirectionPadFragment">

    <ImageView
        android:id="@+id/imageview_topleft_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@id/imageview_middleleft_direction_pad_fragment"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/imageview_topcenter_direction_pad_fragment"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageview_topcenter_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@id/imageview_middlecenter_direction_pad_fragment"
        app:layout_constraintLeft_toRightOf="@id/imageview_topleft_direction_pad_fragment"
        app:layout_constraintRight_toLeftOf="@id/imageview_topright_direction_pad_fragment"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageview_topright_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@id/imageview_middleright_direction_pad_fragment"
        app:layout_constraintLeft_toRightOf="@id/imageview_topcenter_direction_pad_fragment"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageview_middleleft_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@id/imageview_bottomleft_direction_pad_fragment"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/imageview_middlecenter_direction_pad_fragment"
        app:layout_constraintTop_toBottomOf="@id/imageview_topleft_direction_pad_fragment" />

    <ImageView
        android:id="@+id/imageview_middlecenter_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@id/imageview_bottomcenter_direction_pad_fragment"
        app:layout_constraintLeft_toRightOf="@id/imageview_middleleft_direction_pad_fragment"
        app:layout_constraintRight_toLeftOf="@id/imageview_middleright_direction_pad_fragment"
        app:layout_constraintTop_toBottomOf="@id/imageview_topcenter_direction_pad_fragment" />

    <ImageView
        android:id="@+id/imageview_middleright_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@id/imageview_bottomright_direction_pad_fragment"
        app:layout_constraintLeft_toRightOf="@id/imageview_middlecenter_direction_pad_fragment"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageview_topright_direction_pad_fragment" />

    <ImageView
        android:id="@+id/imageview_bottomleft_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/imageview_bottomcenter_direction_pad_fragment"
        app:layout_constraintTop_toBottomOf="@id/imageview_middleleft_direction_pad_fragment" />

    <ImageView
        android:id="@+id/imageview_bottomcenter_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/imageview_bottomleft_direction_pad_fragment"
        app:layout_constraintRight_toLeftOf="@id/imageview_bottomright_direction_pad_fragment"
        app:layout_constraintTop_toBottomOf="@id/imageview_middlecenter_direction_pad_fragment" />

    <ImageView
        android:id="@+id/imageview_bottomright_direction_pad_fragment"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@id/imageview_bottomcenter_direction_pad_fragment"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageview_middleright_direction_pad_fragment" />


</androidx.constraintlayout.widget.ConstraintLayout>