<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout_refrigerator"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/shape_refrigerator"
    tools:context=".sequencetrainer.hotbar.refrigerator.RefrigeratorFragment">

    <com.jackingaming.thestraylightrun.sequencetrainer.hotbar.refrigerator.entities.Milk
        android:id="@+id/milk_coconut"
        android:layout_width="64dp"
        android:layout_height="128dp"
        android:background="@color/green"
        app:layout_constraintEnd_toStartOf="@id/milk_almond"
        app:layout_constraintTop_toTopOf="parent" />

    <com.jackingaming.thestraylightrun.sequencetrainer.hotbar.refrigerator.entities.Milk
        android:id="@+id/milk_almond"
        android:layout_width="64dp"
        android:layout_height="128dp"
        android:background="@color/brown"
        app:layout_constraintEnd_toStartOf="@id/milk_soy"
        app:layout_constraintTop_toTopOf="parent" />

    <com.jackingaming.thestraylightrun.sequencetrainer.hotbar.refrigerator.entities.Milk
        android:id="@+id/milk_soy"
        android:layout_width="64dp"
        android:layout_height="128dp"
        android:background="@color/cream"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>